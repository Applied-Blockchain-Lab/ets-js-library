{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:base"],
  "hostRules": [
    {
      "matchHost": "https://github.com/orgs/Applied-Blockchain-Lab",
      "encrypted": {
        "token": ""
      },
      "hostType": "github"
    }
  ],
  "packageRules": [
    {
      "matchDepTypes": ["dependencies"],
      "rangeStrategy": "pin",
      "enabled": true
    },
    {
      "matchDepTypes": ["devDependencies"],
      "enabled": false
    }
  ]
}
