{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:base"],
  "packageRules": [
    {
      "matchDepTypes": ["dependencies"],
      "rangeStrategy": "bump",
      "enabled": true
    },
    {
      "matchPackageNames": ["event-ticketing-system"],
      "versioning":"git",
      "enabled": true
    },
    {
      "matchDepTypes": ["devDependencies"],
      "enabled": false
    }
  ]
}
